# Copy this file to .env (project root). This file is gitignored.



# Supabase (frontend via Vite)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
# IMPORTANT: do not commit your real service role key
SUPABASE_SERVICE_ROLE_KEY=

SUPABASE_URL=${VITE_SUPABASE_URL}
SUPABASE_ANON_KEY=${VITE_SUPABASE_ANON_KEY}

# Mistral (backend)
MISTRAL_API_KEY=
MISTRAL_MODEL=mistral-small-latest

ADMIN_EMAILS=

# Optional: overrides the default backend system prompt
SYSTEM_PROMPT=

# Billing / Stripe
# Payment Links (used by POST /api/billing/checkout-session)
STRIPE_PAYMENT_LINK_PRO=https://buy.stripe.com/9B6aEW5dr8s9dqj2nt14401
STRIPE_PAYMENT_LINK_TOPUP_250K=https://buy.stripe.com/bJe14mbBP6k171VaTZ14402

# Webhook (required to actually grant Pro / token packs after payment)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Dev-only: instantly grant without Stripe (0/1)
BILLING_DEV_GRANT=0
